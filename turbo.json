{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "bootstrap": {
      "cache": true,
      "dependsOn": ["^bootstrap"],
      "outputs": ["dist/**"]
    },
    "build": {
      "dependsOn": ["build"],
      "outputs": ["dist/**", "build/**"]
    },
    "clean": {
      "cache": false
    }
  }
}
